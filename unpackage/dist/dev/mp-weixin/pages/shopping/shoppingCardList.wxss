.popup-center-view{
		display: flex;
		align-items: center;
		width: 644rpx;
		height: 469rpx;
		background: #FFFFFF;
		border-radius: 10rpx;
		flex-direction: column;
}
.popup-center-view-img{
		width: 68rpx;
		height: 68rpx;
		margin-top: 70rpx;
}
.popup-center-view-text{
		font-size: 30rpx;
		color: #333333;
