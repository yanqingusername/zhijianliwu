
.is-fixed{
		position: fixed!important;
		left: 0;
		right: 0;
		width: 100%;
		background-color: #F9FAFE;
		padding: 24rpx 0 24rpx;
		z-index: 2;
}
.page{
		/* padding-top: 80rpx; */
		background: #F5F5F5;
}
.z-tabar{
		position: relative;
		padding-right: 150rpx;
		padding-left: 26rpx;
		box-sizing: border-box;
}
.owm-index-tab-bar-box{
		width: 100%;
		/* position: absolute; */
		/* top: 1080rpx; */
}
.right-op{
		position: absolute;
		right: 26rpx;
		top: 50%;
		-webkit-transform: translateY(-50%);
		        transform: translateY(-50%);
		background-color: #f5f5f5;
}
.right-op text{
		vertical-align: middle;
		display: inline-block;
		color: #333;
		font-size: 28rpx;
		font-weight: bold;
}
.right-op image{
		width: 40rpx;
		height: 40rpx;
		margin-left: 10rpx;
		vertical-align: middle;
		display: inline-block;
}
