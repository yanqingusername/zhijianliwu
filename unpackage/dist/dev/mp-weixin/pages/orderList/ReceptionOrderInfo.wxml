<view><view class="personal-header" style="{{('height:'+(isSystemInfo?'190':'160')+'px')}}"><view class="my-nav" style="{{('height:'+nav+'px')}}"></view><view class="personal-header-interstall"><image class="icon-back-img" src="../../static/icon_header_back.png" data-event-opts="{{[['tap',[['$buttonClick',['$0'],['backbutton']]]]]}}" bindtap="__e"></image><view class="personal-header-title"></view></view><view class="recharge-status-top"><block wx:if="{{orderReceptionInfo.orderinfo.status==2}}"><image class="recharge-status-img" src="../../static/icon_transit_reception.png"></image></block><block wx:if="{{orderReceptionInfo.orderinfo.status==3}}"><image class="recharge-status-img" src="../../static/icon_completed_reception.png"></image></block><view class="recharge-status-text">{{orderReceptionInfo.orderinfo.order_status_info}}</view></view></view><view class="recharge-flex"><block wx:if="{{orderReceptionInfo.orderinfo.wuliu_info.length>0}}"><view class="reception-flex"><view class="reception-address-view" data-ordernumber="{{orderReceptionInfo.orderinfo.ordernumber}}" data-event-opts="{{[['tap',[['logisticInfo',['$event']]]]]}}" bindtap="__e"><view class="reception-address-view-left"><view class="reception-distribution-value">{{orderReceptionInfo.orderinfo.wuliu_info[0].context}}</view><view class="reception-distribution-time">{{orderReceptionInfo.orderinfo.wuliu_info[0].time}}</view></view><view class="reception-address-view-right"><image class="reception-address-arrow" src="../../static/return_arrow_r_g.png"></image></view></view></view></block><block wx:if="{{orderReceptionInfo.orderinfo.linkman}}"><view class="reception-flex"><view class="reception-address-view" style="margin-top:0rpx;padding-bottom:30rpx;"><view class="reception-address-view-left"><view class="reception-address-name">{{orderReceptionInfo.orderinfo.linkman+" "+orderReceptionInfo.orderinfo.linktel}}</view><view class="reception-address-value">{{orderReceptionInfo.orderinfo.province+orderReceptionInfo.orderinfo.city+orderReceptionInfo.orderinfo.county+orderReceptionInfo.orderinfo.address}}</view></view><view class="reception-address-view-right"></view></view><image class="reception-address-icon" src="../../static/icon_location_reception.png"></image></view></block></view><block wx:if="{{orderReceptionInfo.orderinfo.wuliu_info.length>0||orderReceptionInfo.orderinfo.linkman}}"><view class="order-border"></view></block><view class="order-purchase-view"><view class="new-order-li"><view class="new-order-li-top"><image class="new-order-header-img" lazy-load="true" src="{{orderReceptionInfo.orderinfo.give_head_img}}" mode></image><view class="new-order-nickname">{{orderReceptionInfo.orderinfo.give_name}}</view><view class="new-order-li-top-ordersn">送出的</view></view><block wx:for="{{orderReceptionInfo.orderdetail}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="new-order-li-center"><view class="new-order-left"><view class="new-order-img"><image class="new-order-commodity-img" lazy-load="true" src="{{item.head_img}}" mode></image></view></view><view class="new-order-right"><view class="new-order-item"><view class="new-order-item-title">{{item.goodsname}}</view><view class="new-order-item-money">{{"¥"+item.goods_price}}</view></view><view class="new-order-item"><view class="new-order-item-sku">{{"规格："+item.goods_spec_item}}</view><view class="new-order-item-total">{{"x"+item.goodsnum}}</view></view></view><view class="conversion-details">{{item.cancel_type_info}}</view></view></block><block wx:if="{{orderReceptionInfo.orderinfo.status==3}}"><view class="new-order-li-bottom"><view class="new-order-nickname"></view><view class="new-order-botton-view"><view class="new-order-botton-gray" data-ordernumber="{{orderReceptionInfo.orderinfo.ordernumber}}" data-event-opts="{{[['tap',[['RefundAfterSale',['$event']]]]]}}" bindtap="__e">退换/售后</view></view></view></block></view></view><view class="reception-order"><view class="reception-order-view"><view class="reception-order-text">领取编号：</view><view class="reception-ordersn">{{orderReceptionInfo.orderinfo.ordernumber}}</view><view class="reception-order-copy" data-ordernumber="{{orderReceptionInfo.orderinfo.ordernumber}}" data-event-opts="{{[['tap',[['copy',['$event']]]]]}}" bindtap="__e">复制</view></view><view class="reception-order-view" style="margin-top:12rpx;"><view class="reception-order-text">领取时间：</view><view class="reception-order-time">{{orderReceptionInfo.add_time}}</view></view></view><view class="reception-empty"></view></view>