
.own-product-row{
	font-size: 0!important;
	margin-top: 22rpx;
	padding: 0 16rpx;
	box-sizing: border-box;
}
.own-product-column{
	width: 50%;
	display: inline-block;
	padding: 0 10rpx;
	box-sizing: border-box;
	vertical-align: top;
}
.own-product-bottom{
	/* height: 32rpx; */
	color: #666666;
	text-align: center;
	padding: 20rpx 0;
}
.post-bottom{
		display: flex;
		align-items: center;
		color: #B3B3B3;
		text-align: center;
		padding-top: 24rpx;
		padding-bottom: 48rpx;
		justify-content: center;
}
.post-bottom-text{
		font-size: 24rpx;
		color: #B3B3B3;
		line-height: 33rpx;
		margin: 0rpx 18rpx;
}
.post-bottom-line{
		width: 240rpx;
		height: 1px;
		background: #E0E0E0;
}
.apply-success-view{
		display: flex;
		flex-direction: column;
		align-items: center;
}
.apply-success-img{
		width: 255rpx;
		height: 180rpx;
		margin-top: 172rpx;
}
.apply-success-text{
		padding: 60rpx 80rpx;
		text-align: center;
		font-size: 28rpx;
		color: #666666;
		line-height: 48rpx;
}

