
.own-coupon-list-start{
	border-radius: 50rpx 0 0 50rpx;
	color: #FFFFFF;
	background-color: rgba(255, 1, 55, 1);
	width: 100rpx;
	height: 36rpx;
	line-height: 36rpx;
	text-align: center;
}
.own-coupon-list-layer{
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background-color: rgba(0, 0, 0, 0.3);
	z-index: 998;
}
.own-coupon-list-box{
	width: 100%;
	height: -webkit-max-content;
	height: max-content;
	border-radius: 32rpx 32rpx 0 0;
	background: #FFF;
	-webkit-box-shadow: 0 0 6rpx 0rpx rgba(0, 0, 0, 0.3);
	        box-shadow: 0 0 6rpx 0rpx rgba(0, 0, 0, 0.3);
	position: fixed;
	padding-top: 40rpx;
	top: auto;
	left: 0;
	right: 0;
	bottom: 0;
	z-index: 999;
}
.own-coupon-list{
	width: 90%;
	margin: 0 auto;
}
.own-coupon{
	margin-bottom: 24rpx;
}
.own-coupon-list-button{
	width: 90%;
	margin: 0 auto;
	height: 64rpx;
	line-height: 64rpx;
	color: #fff;
	border-radius: 16rpx;
	text-align: center;
	background-color: rgba(255, 1, 55, 1);
	margin-bottom: 24rpx;
}
.own-coupon-list-button text{
	display: inline-block;
	font-size: 1.2em;
	width: 120rpx;
	text-align-last: justify;
}

