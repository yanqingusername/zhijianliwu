<view><view class="logistics-info-view"><view class="logistics-info-view-title">{{stepsInfo.express_name+"："+stepsInfo.express_num}}</view><view class="logistics-info-view-lable" data-ordernumber="{{stepsInfo.express_num}}" data-event-opts="{{[['tap',[['copy',['$event']]]]]}}" bindtap="__e">复制</view></view><view class="Logistics_information"><block wx:for="{{stepsInfo.wuliu_info}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="Logistics_informationBox"><view class="Logistics_information_left"></view><view class="Logistics_information_right"><view class="imagesimg"><block wx:if="{{item.status_type}}"><view class="wl_Order_placed"><block wx:if="{{item.status_type==1}}"><image class="wl_Order_placed_img" src="../../static/icon_list_logistics_01.png"></image></block><block wx:if="{{item.status_type==2}}"><image class="wl_Order_placed_img" src="../../static/icon_warehouse_logistics_02.png"></image></block><block wx:if="{{item.status_type==3}}"><image class="wl_Order_placed_img" src="../../static/icon_delivered_logistics_03.png"></image></block><block wx:if="{{item.status_type==4}}"><image class="wl_Order_placed_img" src="../../static/icon_transit_logistics_04.png"></image></block><block wx:if="{{item.status_type==5}}"><image class="wl_Order_placed_img" src="../../static/icon_delivery_logistics_05.png"></image></block><block wx:if="{{item.status_type==6}}"><image class="wl_Order_placed_img" src="../../static/icon_signed_logistics_06.png"></image></block><block wx:if="{{item.status_type==7}}"><view style="font-size:20rpx;color:#FFCDCD;width:40rpx;height:40rpx;display:flex;align-items:center;justify-content:center;">收</view></block><block wx:if="{{item.status_type==8}}"><view style="font-size:20rpx;color:#FFCDCD;width:40rpx;height:40rpx;display:flex;align-items:center;justify-content:center;">完</view></block></view></block><block wx:else><view class="wl_Order_placed_view"></view></block></view><view class="content"><view class="content_title">{{item.status}}</view><view class="content_detail">{{item.context}}</view><view class="content_data">{{item.time}}</view><view style="height:40rpx;"></view></view></view></view></block></view></view>