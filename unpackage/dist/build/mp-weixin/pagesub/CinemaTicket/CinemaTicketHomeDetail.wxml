<view><view class="cthd-view" style="{{('background-image: url('+movieInfo.picHd+');')}}"><view class="cthd-b"><view class="cth-b-bg-2-1"><view class="cth-b-bg-2-1-1">{{movieInfo.name}}</view><view class="cth-b-bg-2-1-2"><view class="cth-b-bg-2-1-2-1">3D</view><view class="cth-b-bg-2-1-2-2">IMAX</view></view></view><view class="cth-b-bg-2-2">{{movieInfo.duration+"分钟/"+movieInfo.filmTypes+"/"+movieInfo.movieArea}}</view><view class="cth-b-bg-2-3">{{movieInfo.publishDate+" 在"+movieInfo.movieArea+"上映"}}</view><view class="cth-b-bg-2-4"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><image class="cth-b-bg-2-4-img" src="{{item.imgurl}}"></image></view></block>{{movieInfo.grade+'分'}}</view><view class="cth-b-bg-2-5">参与演员</view><view class="cth-b-bg-2-6">{{movieInfo.cast}}</view></view></view><view style="height:160rpx;width:100%;"></view><view data-event-opts="{{[['tap',[['clickBuy',['$event']]]]]}}" class="cthd-bottom" bindtap="__e"><view class="cthd-bottom-title">购票</view></view></view>