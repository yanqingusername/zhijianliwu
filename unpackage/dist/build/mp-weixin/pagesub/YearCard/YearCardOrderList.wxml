<view><block wx:for="{{screenPurchase}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{screenPurchase.length>0}}"><view class="order-purchase-view"><view class="new-order-li" data-keynum="{{item.clientkeynum}}" data-event-opts="{{[['tap',[['handlerOrderInfo',['$event']]]]]}}" bindtap="__e"><view class="new-order-li-top"><view class="new-order-li-top-ordersn">{{"兑换编号："+item.ordernumber}}</view><view class="new-order-li-top-orderstatus">{{item.wulliu_status_info+" >"}}</view></view><view class="new-order-li-center"><view class="new-order-left"><view class="new-order-img"><image class="new-order-commodity-img" lazy-load="true" src="{{item.head_img}}" mode></image></view><view class="new-order-item"><view class="new-order-item-title">{{item.goodsname}}</view><view class="new-order-right"><view class="new-order-item-sku">{{"规格："+(item.goods_spec_item||'默认')}}</view><view class="new-order-item-total">{{"x"+item.goodsnum}}</view></view></view></view></view><view class="new-order-li-bottom"><view class="new-order-nickname">{{"兑换时间："+item.add_time}}</view><view class="new-order-botton-gray" data-ordernumber="{{item.ordernumber}}" data-event-opts="{{[['tap',[['logisticInfo',['$event']]]]]}}" catchtap="__e">查看物流</view></view></view></view></block></block><view style="height:40rpx;"></view></view>