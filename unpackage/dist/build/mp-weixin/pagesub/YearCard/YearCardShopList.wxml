<view style="border-top:1px solid #F5F5F5;"><block wx:for="{{screenPurchase}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{screenPurchase.length>0}}"><view class="order-purchase-view"><view class="new-order-li" data-keynum="{{item.keynum}}" data-event-opts="{{[['tap',[['handlerOrderInfo',['$event']]]]]}}" bindtap="__e"><view class="new-order-img"><image class="new-order-commodity-img" lazy-load="true" src="{{item.head_img}}" mode></image></view><view class="new-order-item"><view class="new-order-item-title">{{item.goodsname}}</view><view class="new-order-item-sku">{{item.goodstitle}}</view><view class="new-order-right"><view class="new-order-item-sku"><block wx:if="{{item.effective_e_time||item.effective_e_time}}"><view class="new-order-item-sku-time">{{"兑换时间："+item.effective_s_time+"～"+item.effective_e_time}}</view></block></view><block wx:if="{{item.effective_status==1}}"><view class="new-order-item-total" data-keynum="{{item.keynum}}" data-cliid="{{item.client_card_type_goods_id}}" data-event-opts="{{[['tap',[['go_exchange',['$event']]]]]}}" catchtap="__e">立即兑换</view></block><block wx:if="{{item.effective_status==2}}"><view class="new-order-item-total" style="opacity:0.5;">未生效</view></block><block wx:if="{{item.effective_status==3}}"><view class="new-order-item-total" style="background:#CCCCCC;color:#FFFFFF;">已过期</view></block></view></view></view></view></block></block><view style="height:40rpx;"></view></view>