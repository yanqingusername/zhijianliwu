<view><view class="post-search-top flex-vertically"><view data-event-opts="{{[['tap',[['goToSearch',['$event']]]]]}}" class="search-text flex-vertically" bindtap="__e"><icon class="search-icon" type="search" size="14"></icon><input type="text" disabled="true" value="" placeholder="搜索热门礼物"/></view></view><view class="post-list flex-between border-top"><scroll-view class="classifi-left" style="{{'scrollbars:none;'+('height:'+(height+'px')+';')}}" scroll-y="true" show-scrollbar="true"><block wx:for="{{leftList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['classifi-left-li',leftIndex==index?'classifi-left-li-active':'']}}" data-keynum="{{item.keynum}}" data-index="{{index}}" data-event-opts="{{[['tap',[['bind',['$event']]]]]}}" bindtap="__e">{{item.name}}</view></block></scroll-view><view class="classifi-right"><view class="classifi-list"><block wx:for="{{rightList}}" wx:for-item="classifi" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['goToPost',['$0','$1'],[[['rightList','',index,'keynum']],[['rightList','',index,'name']]]]]]]}}" class="classifi-goods" bindtap="__e"><image class="classifi-bottom-img" src="{{classifi.icon}}" mode></image><view class="classifi-number">{{classifi.name}}</view></view></block></block></view></view></view><view data-event-opts="{{[['tap',[['goToScrollTop',['$event']]]]]}}" class="post-scroll-top" bindtap="__e"><image class="post-scroll-top-img" src="/static/post-scroll-top-img.png" mode="widthFix"></image></view></view>