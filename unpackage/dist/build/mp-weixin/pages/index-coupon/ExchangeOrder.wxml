<view><block wx:for="{{screenPurchase}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{screenPurchase.length>0}}"><view class="order-purchase-view"><view class="new-order-li" data-ordernumber="{{item.ordernumber}}" data-isnianka="{{item.is_nianka}}" data-event-opts="{{[['tap',[['handlerOrderInfo',['$event']]]]]}}" bindtap="__e"><view class="new-order-li-top"><view class="new-order-li-top-ordersn">{{"兑换编号："+item.ordernumber}}</view><block wx:if="{{item.order_status_info}}"><view class="new-order-li-top-orderstatus">{{item.order_status_info+" >"}}</view></block></view><view class="new-order-li-center"><view class="new-order-left"><view class="new-order-img"><image class="new-order-commodity-img" lazy-load="true" src="{{item.cardtype_img}}" mode></image></view><view class="new-order-item"><view class="new-order-item-title">{{item.card_name}}</view><block wx:if="{{item.card_type_info}}"><view class="new-order-item-sku">{{"规格："+item.card_type_info}}</view></block></view></view><view class="new-order-right"><view class="new-order-item-total">共1件</view></view></view><block wx:if="{{item.order_status_type!=4}}"><view class="new-order-li-bottom"><view class="new-order-nickname"></view><block wx:if="{{item.card_type==0}}"><view class="new-order-botton-view"><block wx:if="{{item.order_status_type==0}}"><view class="new-order-botton-gray" data-ordernumber="{{item.ordernumber}}" data-event-opts="{{[['tap',[['goTransfer',['$event']]]]]}}" catchtap="__e">转赠</view></block><block wx:if="{{item.order_status_type==0}}"><view class="new-order-botton" data-ordernumber="{{item.ordernumber}}" data-event-opts="{{[['tap',[['goRecharge',['$event']]]]]}}" catchtap="__e">去充值</view></block></view></block><block wx:if="{{item.card_type==1}}"><view class="new-order-botton-view"><block wx:if="{{item.is_nianka==0}}"><view class="new-order-botton-view"><block wx:if="{{item.order_status_type==0&&item.is_exchange==0}}"><view class="new-order-botton-gray" data-ordernumber="{{item.ordernumber}}" data-event-opts="{{[['tap',[['goTransfer',['$event']]]]]}}" catchtap="__e">转赠</view></block><block wx:if="{{item.order_status_type==0}}"><view class="new-order-botton" data-cardid="{{item.cardid}}" data-isnianka="{{item.is_nianka}}" data-ordernumber="{{item.ordernumber}}" data-event-opts="{{[['tap',[['go_exchange',['$event']]]]]}}" catchtap="__e">去兑换</view></block><block wx:if="{{(item.order_status_type==1||item.order_status_type==2||item.order_status_type==3)&&item.is_nianka==0}}"><view class="new-order-botton-gray" data-ordernumber="{{item.ordernumber}}" data-event-opts="{{[['tap',[['refundHandler',['$event']]]]]}}" catchtap="__e">换货/售后</view></block></view></block><block wx:else><view class="new-order-botton-view"><block wx:if="{{item.order_status_type==0&&item.is_exchange==0}}"><view class="new-order-botton-gray" data-ordernumber="{{item.ordernumber}}" data-event-opts="{{[['tap',[['goTransfer',['$event']]]]]}}" catchtap="__e">转赠</view></block><block wx:if="{{item.order_status_type==0&&item.is_cardbag_logo==0}}"><view class="new-order-botton" data-cardid="{{item.cardid}}" data-isnianka="{{item.is_nianka}}" data-ordernumber="{{item.ordernumber}}" data-event-opts="{{[['tap',[['go_exchange',['$event']]]]]}}" catchtap="__e">去兑换</view></block></view></block></view></block></view></block></view></view></block></block><block wx:if="{{isAll&&screenPurchase.length>0}}"><view class="post-bottom"><view class="post-bottom-line"></view><view class="post-bottom-text">我是有底线的</view><view class="post-bottom-line"></view></view></block><block wx:if="{{screenPurchase.length==0}}"><view class="apply-success-view"><image class="apply-success-img" src="https://zhijianlw.com/static/web/img/empty_page_xm.png"></image><view class="apply-success-text">当前兑换订单暂无数据～</view></view></block><view style="height:40rpx;"></view></view>